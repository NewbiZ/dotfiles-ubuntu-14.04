XTerm*transparent:        true
XTerm*shading:            33
XTerm*tintColor:          turquoise
!black
XTerm*color0:             #171717
XTerm*color8:             #737373
!red
XTerm*color1:             #FF5E5E
XTerm*color9:             #FF7878
!green
XTerm*color2:             #9CE82B
XTerm*color10:            #9ACD32
!yellow
XTerm*color3:             #F0E68C
XTerm*color11:            #EEE8AA
!blue
XTerm*color4:             #008AFF
XTerm*color12:            #4F98FF
!magenta
XTerm*color5:             #E88CFF
XTerm*color13:            #EDA6FF
!cyan
